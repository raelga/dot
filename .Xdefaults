!! Sauce Code Pro
URxvt.font:             xft:Source Code Pro for Powerline-10:pixelsize=14:style=Light;hinting=True:antialias=True
URxvt.boldFont:         xft:Source Code Pro for Powerline-10:bold:pixelsize=14:hinting=True:antialias=True

! settings
URxvt*saveLines: 8000
URxvt*scrollBar: False
URxvt*scrollTtyOutput: False
URxvt*scrollTtyKeypress: True
URxvt*secondaryScroll: True
URxvt.perl-ext-common : default,matcher,clipboard
URxvt.urlLauncher     : x-www-browser
URxvt.matcher.button  : 1
URxvt*letterSpace: -1
URxvt.keysym.M-c:   perl:clipboard:copy
URxvt.keysym.M-v:   perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
